
var img, x, y;

function preload() {

img = loadImage("sunset.jpeg");

}



function setup() {

createCanvas (600, 1000);

background(0);

noStroke();

}



function draw() {

x = random(width);

y = random(height);

var c = img.get(x, y);

fill(c[0], c[1], c[2], c[3], c[4], 200);

rect (x, y, 50, 30);

}
